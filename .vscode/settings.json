{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#69eeba",
    "activityBar.background": "#69eeba",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#bd70ef",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#69eeba",
    "statusBar.background": "#3be9a5",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#18d98d",
    "statusBarItem.remoteBackground": "#3be9a5",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#3be9a5",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#3be9a599",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#3be9a5",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.requireConfig": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
